@using ZiZhuJY.Web.UI.Helpers
@{
    const string THIS_FILE_PATH = "~/Views/FunctionGraffiti/Index.cshtml";
    ViewBag.Title = HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "PageTitle");
    ViewBag.ApplicationName = HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ApplicationName");
    ViewBag.ApplicationNameRemark = HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ApplicationNameRemark"); 
}
@section HeaderScripts {
    <meta name="keywords" content="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "SeoKeywords")" />
    <meta name="description" content="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "SeoDescription")" />
    <meta name="robots" content="index, follow" />
    <link href="@Url.Content("~/Content/css/functionGraffitiStyle.css?minify=true")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/syntaxhighlighter_3.0.83/styles/shCore.css?minify=true")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/syntaxhighlighter_3.0.83/styles/shThemeDefault.css?minify=true")" rel="Stylesheet" type="text/css" />
    <!--[if lte IE 8]><script language="javascript" type="text/javascript" src="@Url.Content("~/Scripts/flot/excanvas.min.js")"></script><![endif]-->
    @Html.RenderJavaScripts("~/Scripts/jquery.colorhelpers.js")
    <script src="@Url.Content("~/Scripts/flot/flot/jquery.flot.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/flot/flot/jquery.flot.resize.js")" type="text/javascript"></script>
    <script language="javascript" type="text/javascript" src="@Url.Content("~/Scripts/flot/flot/jquery.flot.navigate.js")"></script>
    <script src="@Url.Content("~/Scripts/FunctionGraffiti/jGraffiti-Math.js?minify=true")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FunctionGraffiti/jGraffiti.js?minify=true")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/editarea_0_8_2/edit_area/edit_area_full.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FunctionGraffiti/jGraffiti-Editor.js?minify=true")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/syntaxhighlighter_3.0.83/scripts/shCore.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/syntaxhighlighter_3.0.83/scripts/shBrushJScript.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FunctionGraffiti/FunctionGraffiti.js?minify=true")" type="text/javascript"></script>
    <script type="text/javascript">
    <!--
        $(document).ready(function() {
            functionGraffiti("@System.Threading.Thread.CurrentThread.CurrentUICulture.Name.ToLower().Replace('-', '_')", "@System.Threading.Thread.CurrentThread.CurrentUICulture.Name.ToLower().Substring(0, 2)");
        });
    //-->
    </script>
}

<div id="divLeft">
    <div style="">@Html.ActionLink(HttpContext.GetLocalResourceObject(this.VirtualPath, "NewVersion").ToString(), "Index")</div>
    <div id="divGraffiti">
    </div>
    <p><input type="checkbox" value="Show Legend" id="chkLegend" /> <label for="chkLegend">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ShowLegend")</label></p>
</div>
<div id="divTools">
    <p>
        <label for="txtMinOfx">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ScaleOfXAxiesTitle")</label><input type="text" id="txtMinOfx" value="@ViewBag.MinOfx" />
        <label for="txtMaxOfx" style="width: 1em;">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ToTitle")</label>
        <input type="text" id="txtMaxOfx" value="@ViewBag.MaxOfx" /></p>
    <p>
        <label for="txtMinOfy">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ScaleOfYAxiesTitle")</label><input type="text" id="txtMinOfy" value="@ViewBag.MinOfy" />
        <label for="txtMaxOfy" style="width: 1em;">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ToTitle")</label>
        <input type="text" value="@ViewBag.MaxOfy" id="txtMaxOfy" />
        <input type="checkbox" value="@ViewBag.AutoYrange" id="chkAutoYRange" /> <label for="chkAutoYRange">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "AutoYRangeLabel")</label>
    </p>
    <p>
        <label for="txtMinOft">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ScaleOfTAxiesTitle")</label><input type="text" id="txtMinOft" value="@ViewBag.MinOft" />
        <label for="txtMaxOft" style="width: 1em;">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ToTitle")</label>
        <input type="text" value="@ViewBag.MaxOft" id="txtMaxOft" /></p>
    <p>
        <label for="txtPointsCount">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "PointsCountTitle")</label><input type="text" id="txtPointsCount" value="@ViewBag.Points" />
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "PointsCountTip")</p>
    <p>
        <label for="taExpression">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "FunctionsTitle")</label> <span style="float: right;">@Html.Raw(HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ListOfFunctions").ToString())</span></p>
    <textarea id="taExpression" rows="12" cols="40">@ViewBag.Functions</textarea>
    <p class="tip">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Tip")</p>
    <p>
        <input id="btnGraph" type="submit" value="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "GraphButtonText")" accesskey="G" />
        <input type="reset" value="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ResetButtonText")" id="btnReset" accesskey="R" />
        @*<input type="button" id="btnShare" value="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ShareButtonText")" accesskey="S" /> *@
        <input type="button" id="btnLink" value="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "LinkButtonText")" accesskey="L" />
        <br />
        <br />
        <label for="txtLink" style="width: auto;">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "LinkLabelText")</label>
        <input type="text" id="txtLink" value="@Request.Url" style="width: 100%;" /></p>
</div>
<div id="divInstructions">
    <h3>@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InstructionsTitle")</h3>
    <p>@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InstructionsDesc1")</p>
    <p>@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InstructionsDesc2")</p>
    <p>@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InstructionsDesc3")</p>
</div>
<div class="clearFloat">
</div>

<div id="divMisc">
    @Html.Raw(HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Misc").ToString())
    <hr />
    @Html.Raw(HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ThirdPartyCommentCode").ToString())
    <hr />
    @Html.Raw(HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Ad").ToString())
</div>

<div id="help" class="tip leftPanel">
    @Html.Raw(HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Samples").ToString())
    <p>
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "TitleOfWaterStream") <a href="javascript:jGraffiti.graph('y = 1 - 23.45 / pow(x, 4)', -5, 5, -5, 2, 0, 'PI * 2.1', 500, false);"
            title="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "TitleOfWaterStream")" target="_self">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ClickToSeeGraph")</a><span style="float: right;">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Thanks") <a href="http://www.newsenst.com/post/109.html"
                title="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "LiXinBlog")">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "LiXinBlog")</a> @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Provide")</span></p>
    <p style="text-indent: 2em;">
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InputFunctions")</p>
    <pre class="brush: javascript">y = 1 - 23.45 / pow(x, 4)</pre>
    <p>
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "TightEncirclement") <a href="javascript:jGraffiti.graph('{\n\tx = 4*cos(7*t);\n\ty = 4*sin(5*t)\n}', -5, 5, -5, 5, 0, 'PI * 2.1', 500);"
            title="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "TightEncirclement")" target="_self">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ClickToSeeGraph")</a><span style="float: right;">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Thanks") <a href="http://www.newsenst.com/post/109.html"
                title="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "LiXinBlog")">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "LiXinBlog")</a> @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Provide")</span></p>
    <p style="text-indent: 2em;">
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InputFunctions")</p>
    <pre class="brush: javascript">{
	x = 4*cos(7*t); 
	y = 4*sin(5*t)
}</pre>
    <p>
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Spiral") <a href="javascript:jGraffiti.graph('{\n\tx = cos(t)*t/3;\n\ty = sin(t)*t/3\n}', -15, 15, -15, 15, 0, 'PI * 10', 500);"
            title="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Spiral") " target="_self">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ClickToSeeGraph")</a></p>
    <p style="text-indent: 2em;">
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InputFunctions")</p>
    <pre class="brush: javascript">{
	x = cos(t)*t/3;
	y = sin(t)*t/3
}</pre>
    <p>
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Darts") <a href="javascript:jGraffiti.graph('{\n\tx = 2 * (5*cos(t) - cos(6*t));\n\ty = 2 * (3*sin(t) - sin(4*t))\n}', -15, 15, -15, 15, 0, 'PI * 2', null);"
            title="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Darts")" target="_self">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ClickToSeeGraph")</a></p>
    <p style="text-indent: 2em;">
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InputFunctions")</p>
    <pre class="brush: javascript">{
	x = 2 * (5*cos(t) - cos(6*t));
	y = 2 * (3*sin(t) - sin(4*t))
}</pre>
    <p>
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Nappe")<a href="javascript:jGraffiti.graph('y = pow(x, 2) - 4 - pow(x, 4) /10 + 3 * pow(x, 6)/1000;', -6, 6, -5, 5, 0, 'PI * 2', null, false);"
            title="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Nappe")" target="_self">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ClickToSeeGraph")</a></p>
    <p style="text-indent: 2em;">
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InputFunctions")</p>
    <pre class="brush: javascript">y = pow(x, 2) - 4 - pow(x, 4) / 10 + 3 * pow(x, 6) / 1000</pre>
    <p>
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "CharmingCurve") <a href="javascript:jGraffiti.graph('x = 0;\n{x = cos(t); y = asin(t)}\n{x = -cos(t); y = asin(t)}', -1.2, 1.2, -1.2, 1.2, '-PI * 2.5', 'PI * 2.5', null, false);"
            title="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "CharmingCurve")" target="_self">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ClickToSeeGraph")</a></p>
    <p style="text-indent: 2em;">
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InputFunctions")</p>
    <pre class="brush: javascript">x = 0;
{x = cos(t); y = asin(t)}
{x = -cos(t); y = asin(t)}</pre>
    <p>
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Smoke") <a href="javascript:jGraffiti.graph('{' + '\n' + 'x = 0.5*pow(t, 1.5) + 0.2*sin(0.5*t) + sin(t); ' + '\n' + 'y = 0.5*t + 1.5*sin(0.2*t) + 0.5*sin(t)' + '\n' + '}', 0, 80, 0, 15, '-10*PI', '10*PI', null, false);"
            title="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Smoke")" target="_self">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ClickToSeeGraph")</a></p>
    <p style="text-indent: 2em;">
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InputFunctions")</p>
    <pre class="brush: javascript">{
x = 0.5*pow(t, 1.5) + 0.2*sin(0.5*t) + sin(t);
y = 0.5*t + 1.5*sin(0.2*t) + 0.5*sin(t)
}</pre>
    <p>
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Jiong") <a href="javascript:jGraffiti.graph('(4*pow(x, 2) + 5*x - 1)/(pow(x, 2) + x - 3);', -6, 5, -4, 9, null, null, null, false);"
            title="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Jiong")" target="_self">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ClickToSeeGraph")</a></p>
    <p style="text-indent: 2em;">
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InputFunctions")</p>
    <pre class="brush: javascript">(4*pow(x, 2) + 5*x - 1)/(pow(x, 2) + x - 3);</pre>
    <p>
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Spaghetti") <a href="javascript:jGraffiti.graph('sin(x+1/x);', -Math.PI * 2, Math.PI * 2, -5, 5, null, null, null);"
            title="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Spaghetti")" target="_self">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ClickToSeeGraph")</a></p>
    <p style="text-indent: 2em;">
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InputFunctions")</p>
    <pre class="brush: javascript">sin(x+1/x);</pre>
    <p>
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Circle") <a href="javascript:jGraffiti.graph('{x = cos(t); y = sin(t)}', -1.2, 1.2, -1.2, 1.2, 0, 'PI * 2', null);"
            title="@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "Circle")" target="_self">@HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "ClickToSeeGraph")</a></p>
    <p style="text-indent: 2em;">
        @HttpContext.GetLocalResourceObject(THIS_FILE_PATH, "InputFunctions")</p>
    <pre class="brush: javascript">
{x = cos(t); y = sin(t)}</pre>
</div>